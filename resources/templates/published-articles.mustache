{{#published-articles}}
    <li>
        <a href="{{link}}">{{title}}</a>
        <ul class="meta">
            <li class="publish-date">{{publish-date}}</li>
            <li class="publisher">{{publisher}}</li>
            {{#reader-fav}}
                <li class="readers-fav">readersâ€™ fav</li>
            {{/reader-fav}}

            
        </ul>
        <ul class="tweets">
            {{#tweets}}
                <li class="tweet">
                    <span class="icon-twitter"></span>
                    <a href="http://twitter.com/{{screen-name}}/statuses/{{id}}" class="tweet-permalink">
                        <span class="tweet-author-name">{{author-name}}</span>
                        <span class="tweet-screen-name">@{{screen-name}}</span>
                    </a>
                    <p class="tweet-html">{{{html}}}</p>
                </li>
            {{/tweets}}
        </ul>

    </li>
{{/published-articles}}